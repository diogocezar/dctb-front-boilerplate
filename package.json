{
	"name": "dctb-front-boilerplate",
	"version": "1.0.0",
	"description": "Just a simple boilerplate to front-end stuffs using Gulp, Babel and Browserify",
	"main": "gulpfile.js",
	"scripts": {
		"serve": "gulp serve",
		"image": "gulp image",
		"svg": "gulp svg",
		"sass": "gulp sass",
		"js": "gulp js",
		"watch": "gulp watch",
		"build": "gulp"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/diogocezar/dctb-front-boilerplate.git"
	},
	"keywords": [
		"babel",
		"browserify",
		"gulp",
		"modernjs",
		"front-end",
		"sass"
	],
	"author": "Diogo Cezar <diogo@diogocezar.com>",
	"license": "Apache",
	"bugs": {
		"url": "https://github.com/diogocezar/dctb-front-boilerplate/issues"
	},
	"homepage": "https://github.com/diogocezar/dctb-front-boilerplate#readme",
	"browserify": {
		"transform": [
			[
				"babelify",
				{
					"presets": [
						"es2015"
					]
				}
			]
		]
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-preset-es2015": "^6.24.1",
		"babelify": "^8.0.0",
		"bootstrap": "^4.1.1",
		"browser-sync": "^2.24.5",
		"browserify": "^16.2.2",
		"gulp": "^3.9.1",
		"gulp-babel": "^7.0.1",
		"gulp-color": "0.0.2",
		"gulp-cssmin": "^0.2.0",
		"gulp-imagemin": "^4.1.0",
		"gulp-rename": "^1.3.0",
		"gulp-sass": "^4.0.1",
		"gulp-svgo": "^1.5.4",
		"gulp-uglify": "^3.0.0",
		"jquery": "^3.3.1",
		"jquery.nicescroll": "^3.7.6",
		"owl.carousel": "^2.3.4",
		"popper.js": "^1.14.3",
		"scrollreveal": "^3.4.0",
		"vinyl-buffer": "^1.0.1",
		"vinyl-source-stream": "^2.0.0"
	},
	"dependencies": {}
}
